@ARTICLE{Hamrick2005,
  author = {Hamrick, R.G. and Pirgalioglu, T. and Gunduz, S. and Carroll, J.P.},
  title = {Feral donkey \textit{Equus asinus} populations on the Karpaz peninsula,
